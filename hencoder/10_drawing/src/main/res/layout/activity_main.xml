            <com.hsicen.drawing.test.ColorTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:text="重庆市" />

            <com.hsicen.drawing.test.ColorTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:text="河北省" />

            <com.hsicen.drawing.test.ColorTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
